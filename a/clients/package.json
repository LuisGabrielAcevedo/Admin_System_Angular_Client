{
	"name": "itau-mercury-platform",
	"version": "3.0.42",
	"private": true,
	"scripts": {
		"ng": "ng",
		"tsc": "tsc",
		"postinstall": "npm run generators",
		"prestart": "npm run generators",
		"start": "ng serve",
		"lint": "ng lint",
		"pretest": "npm run generators",
		"pretest-ci": "npm run generators",
		"test": "ng test --watch",
		"test-ci": "ng test client --codeCoverage=true",
		"prebuild": "npm run generators",
		"build": "ng build",
		"prebuild-ci": "npm run generators",
		"build-ci": "npm run build -- --prod",
		"start-prod": "npm run serve -- --prod",
		"doc": "compodoc -p tsconfig.app.json",
		"stats": "npm run build-ci -- --stats-json && webpack-bundle-analyzer dist/client/stats-es2015.json",
		"updateVersionMinor": "npm version patch",
		"generators": "npm run buildEnv && npm run refreshVersion",
		"buildEnv": "node scripts/build-env",
		"refreshVersion": "node scripts/build-version",
		"serve": "ng serve",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run tsc && npm run lint",
			"pre-push": "npm run build-ci && npm run test-ci"
		}
	},
	"dependencies": {
		"@angular/animations": "8.2.14",
		"@angular/cdk": "8.2.3",
		"@angular/common": "8.2.14",
		"@angular/compiler": "8.2.14",
		"@angular/core": "8.2.14",
		"@angular/flex-layout": "8.0.0-beta.27",
		"@angular/forms": "8.2.14",
		"@angular/material": "8.2.3",
		"@angular/platform-browser": "8.2.14",
		"@angular/platform-browser-dynamic": "8.2.14",
		"@angular/router": "8.2.14",
		"@ngu/carousel": "1.5.5",
		"@ngx-translate/core": "12.1.2",
		"@ngx-translate/http-loader": "4.0.0",
		"@types/lodash": "4.14.149",
		"@voxel/internet": "2.1.1",
		"@voxel/mobile": "2.4.4",
		"date-fns": "2.11.1",
		"hammerjs": "2.0.8",
		"lodash": "4.17.15",
		"ng-sidebar": "9.2.0",
		"ng2-currency-mask": "5.3.1",
		"ng2-tooltip-directive": "2.9.20",
		"normalize.css": "8.0.1",
		"rxjs": "6.5.4",
		"tslib": "1.11.1",
		"zone.js": "0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.803.26",
		"@angular/cli": "8.3.26",
		"@angular/compiler-cli": "8.2.14",
		"@angular/language-service": "8.2.14",
		"@babel/core": "7.9.0",
		"@compodoc/compodoc": "1.1.11",
		"@storybook/addon-actions": "5.3.18",
		"@storybook/addon-knobs": "5.3.18",
		"@storybook/addon-links": "5.3.18",
		"@storybook/addon-notes": "5.3.18",
		"@storybook/addons": "5.3.18",
		"@storybook/angular": "5.3.18",
		"@types/jasmine": "3.5.10",
		"@types/jasminewd2": "2.0.8",
		"@types/node": "13.11.0",
		"awesome-typescript-loader": "5.2.1",
		"babel-loader": "8.1.0",
		"codelyzer": "5.2.2",
		"husky": "3.1.0",
		"jasmine-core": "3.5.0",
		"jasmine-spec-reporter": "5.0.1",
		"karma": "4.4.1",
		"karma-chrome-launcher": "3.1.0",
		"karma-coverage-istanbul-reporter": "2.1.1",
		"karma-jasmine": "3.1.1",
		"karma-jasmine-html-reporter": "1.5.3",
		"ts-node": "8.8.1",
		"tslint": "6.1.1",
		"typescript": "3.5.3",
		"webpack-bundle-analyzer": "3.6.1"
	}
}
