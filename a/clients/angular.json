{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"client": {
			"root": "projects",
			"sourceRoot": "projects",
			"projectType": "application",
			"prefix": "mcy",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/client",
						"index": "projects/client/index.html",
						"main": "projects/client/index.ts",
						"polyfills": "projects/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": false,
						"assets": [
							"projects/client/favicon.ico",
							"projects/client/assets",
							"projects/libs/main/assets"
						],
						"styles": [
							"node_modules/normalize.css/normalize.css",
							"node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"projects/libs/main/styles/main.scss",
							"projects/libs/core/styles/core.scss",
							"projects/client/app/root.styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"projects/libs/main/styles"
							]
						}
					},
					"configurations": {
						"production": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "client:build",
						"proxyConfig": "proxy.conf.json"
					},
					"configurations": {
						"production": {
							"browserTarget": "client:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "client:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/client/test.ts",
						"polyfills": "projects/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "projects/client/karma.conf.js",
						"codeCoverage": false,
						"codeCoverageExclude": [
							"projects/environments/**",
							"projects/libs/main/polyfills/**",
							"projects/libs/core/services/analytics.ts",
							"projects/polyfills.ts",
							"projects/test.ts",
							"projects/**/*mock.ts"
						],
						"assets": [
							"projects/client/favicon.ico",
							"projects/client/assets",
							"projects/libs/main/assets"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"projects/libs/main/styles"
							]
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"tsconfig.app.json",
							"tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		},
		"backoffice": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects",
			"sourceRoot": "projects",
			"prefix": "mcy-back",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/backoffice",
						"index": "projects/backoffice/src/index.html",
						"main": "projects/backoffice/src/main.ts",
						"polyfills": "projects/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"aot": false,
						"assets": [
							"projects/backoffice/src/favicon.ico",
							"projects/backoffice/src/assets",
							"projects/libs/main/assets"
						],
						"styles": [
							"node_modules/normalize.css/normalize.css",
							"node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"projects/libs/main/styles/main.scss",
							"projects/libs/core/styles/core.scss",
							"projects/backoffice/src/styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"projects/libs/main/styles"
							]
						}
					},
					"configurations": {
						"production": {
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "backoffice:build",
						"proxyConfig": "proxy.conf.json"
					},
					"configurations": {
						"production": {
							"browserTarget": "backoffice:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "backoffice:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/backoffice/src/test.ts",
						"polyfills": "projects/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "projects/backoffice/src/karma.conf.js",
						"codeCoverage": false,
						"codeCoverageExclude": [
							"projects/environments/**",
							"projects/libs/**",
							"projects/polyfills.ts",
							"projects/test.ts",
							"projects/**/*mock.ts"
						],
						"assets": [
							"projects/backoffice/src/favicon.ico",
							"projects/backoffice/src/assets",
							"projects/libs/main/assets"
						],
						"styles": [
							"projects/backoffice/src/styles.scss"
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"projects/libs/main/styles"
							]
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"tsconfig.app.json",
							"tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		}
	},
	"defaultProject": "client",
	"cli": {
		"analytics": false
	}
}
