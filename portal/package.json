{
  "name": "portalsanrio",
  "version": "2.0.0",
  "publishConfig": {
    "registry": "http://artifacts.intranet/repository/npm-releases/santander-rio/"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "ng build --prod",
    "build": "ng build",
    "format:write": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write --list-different",
    "format:test": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "docs:generate": "compodoc -p src/tsconfig.app.json",
    "docs:server": "http-server documentation/ -o -p 1984",
    "docs": "npm run docs:generate && npm run docs:server",
    "publish:prod": "cp package.json dist && cd dist && npm publish",
    "storybook": "start-storybook -p 6006",
    "storybook:generate": "build-storybook -c .storybook -o stories",
    "storybook:server": "http-server stories/ -o -p 1985",
    "registry": "npm config get registry",
    "registry:nexus": "npm config delete registry && npm config set registry=http://artifacts.intranet/repository/npm-group/",
    "registry:npm": "npm config delete registry"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build:prod",
      "pre-commit": "npm run format:write"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/setupJest.ts",
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ]
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "@ng-select/ng-select": "^2.16.2",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@storybook/addon-knobs": "^5.1.11",
    "@swimlane/ngx-charts": "^10.1.0",
    "@swimlane/ngx-datatable": "^14.0.0",
    "@types/jest": "^24.0.15",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "moment": "^2.24.0",
    "ng2-nouislider": "^1.7.13",
    "ng2-pdf-viewer": "^5.3.2",
    "ngx-currency": "^1.5.2",
    "ngx-mask": "7.7.2",
    "ngx-moment": "^3.4.0",
    "nouislider": "^13.1.4",
    "pdfjs-dist": "^2.0.943",
    "rxjs": "~6.3.3",
    "rxjs-marbles": "^5.0.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@babel/core": "^7.5.5",
    "@compodoc/compodoc": "^1.1.9",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-backgrounds": "5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addon-notes": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/angular": "5.1.9",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-loader": "^8.0.6",
    "codelyzer": "~4.5.0",
    "cypress": "^3.3.1",
    "http-server": "^0.9.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.3.0",
    "open-cli": "^5.0.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  }
}
